Proceso Taller_PSEINT_1_2
	//Realizar un programa el cual solicite su nombre, apellidos, edad y estatura
	Definir nombre Como Caracter;
	Definir edad Como Entero;
	Definir estatura Como Real;
	Escribir "Ingrese su nombre y apellidos";
	Leer nombre;
	Escribir "Ingrese su edad";
	Leer edad;
	Escribir "Ingrese su estatura";
	Leer estatura;
	Escribir "Los datos ingresados son: ";
	Escribir nombre," ", edad," ", estatura;
FinProceso
